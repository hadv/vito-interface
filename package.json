{
  "name": "vito-interface",
  "version": "1.0.0",
  "description": "A secure and efficient application designed to interact with safe{wallet} through a minimalist Vito interface",
  "main": "index.js",
  "scripts": {
    "start": "cd client && npm start",
    "dev": "cd client && npm start",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "cd client && npm test",
    "test:ci": "cd client && npm run test:ci",
    "deploy:ipfs": "./scripts/deploy-ipfs.sh",
    "ens:manager": "node scripts/ens-manager.js",
    "setup:ipfs": "npm install -g @pinata/cli @fleek-platform/cli",
    "setup:ens": "npm install ethers bs58"
  },
  "keywords": [
    "safe-wallet",
    "vito-interface",
    "ethereum",
    "static-app"
  ],
  "author": "",
  "license": "MIT"
}
